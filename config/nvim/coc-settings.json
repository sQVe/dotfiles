{
  "coc.source.emoji.filetypes": ["gitcommit", "mail", "markdown", "markdown.mdx", "text"],
  "diagnostic.checkCurrentLine": true,
  "diagnostic.errorSign": "■",
  "diagnostic.infoSign": "■",
  "diagnostic.warningSign": "■",
  "emmet.excludeLanguages": [
    "javascriptreact",
    "markdown",
    "typescriptreact"
  ],
  "git.addedSign.hlGroup": "GruvboxGreenSign",
  "git.changeRemovedSign.hlGroup": "GruvboxRedSign",
  "git.changedSign.hlGroup": "GruvboxYellowSign",
  "git.removedSign.hlGroup": "GruvboxRedSign",
  "git.topRemovedSign.hlGroup": "GruvboxRedSign",
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    }
  },
  "snippets.priority": 80,
  "snippets.snipmate.enable": false,
  "suggest.completionItemKindLabels": {
    "class": "\uf0e8",
    "color": "\ue22b",
    "constant": "\uf8fe",
    "default": "\uf29c",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "event": "\ufacd",
    "field": "\uf9be",
    "file": "\uf723",
    "folder": "\uf114",
    "function": "\u0192",
    "interface": "\uf417",
    "keyword": "\uf1de",
    "method": "\uf09a",
    "module": "\uf40d",
    "operator": "\u03a8",
    "property": "\ue624",
    "reference": "\ufa46",
    "snippet": "\ue60b",
    "struct": "\ufb44",
    "text": "\ue612",
    "typeParameter": "\uf728",
    "unit": "\uf475",
    "value": "\uf89f",
    "variable": "\ue79b"
  },
  "suggest.languageSourcePriority": 100,
  "suggest.snippetIndicator": " \uf78c",
  "typescript.suggestionActions.enabled": false
}
