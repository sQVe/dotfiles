<?xml version="1.0" encoding="UTF-8"?>

<style-scheme id="sqve" _name="sQVe" version="1.0">
  <author>Oskar 'sQVe' Grunning</author>
  <_description>Catppuccin Latte (Muted) color scheme</_description>

  <color name="rosewater"   value="#8b4d4d"/>
  <color name="flamingo"    value="#a24040"/>
  <color name="pink"        value="#984080"/>
  <color name="mauve"       value="#7c32a8"/>
  <color name="red"         value="#b82530"/>
  <color name="maroon"      value="#a03050"/>
  <color name="peach"       value="#a85820"/>
  <color name="yellow"      value="#806010"/>
  <color name="green"       value="#287028"/>
  <color name="teal"        value="#207068"/>
  <color name="sky"         value="#1878a8"/>
  <color name="sapphire"    value="#206090"/>
  <color name="blue"        value="#2850a8"/>
  <color name="lavender"    value="#5858a0"/>
  <color name="text"        value="#4c4e5e"/>
  <color name="subtext1"    value="#5a5c6a"/>
  <color name="subtext0"    value="#686a78"/>
  <color name="overlay2"    value="#767885"/>
  <color name="overlay1"    value="#848692"/>
  <color name="overlay0"    value="#92949f"/>
  <color name="surface2"    value="#a0a2ab"/>
  <color name="surface1"    value="#afb1b8"/>
  <color name="surface0"    value="#bdbfc5"/>
  <color name="base"        value="#eff1f5"/>
  <color name="mantle"      value="#e6e9ef"/>
  <color name="crust"       value="#dce0e8"/>

  <style name="text"                        foreground="text" background="base"/>
  <style name="selection"                   foreground="text" background="surface1"/>
  <style name="cursor"                      foreground="rosewater"/>
  <style name="current-line"                background="mantle"/>
  <style name="line-numbers"                foreground="overlay1" background="mantle"/>
  <style name="draw-spaces"                 foreground="overlay0"/>

  <style name="bracket-match"               background="surface1"/>
  <style name="bracket-mismatch"            background="red" bold="true"/>

  <style name="search-match"                foreground="base" background="yellow"/>

  <style name="bookmark"                    background="lavender"/>

  <style name="def:comment"                 foreground="overlay1" italic="true"/>
  <style name="def:shebang"                 foreground="pink" italic="true"/>
  <style name="def:doc-comment-element"     bold="true"/>

  <style name="def:constant"                foreground="peach"/>
  <style name="def:string"                  foreground="green"/>
  <style name="def:special-char"            foreground="pink"/>
  <style name="def:special-constant"        foreground="peach" bold="true"/>
  <style name="def:number"                  foreground="peach"/>
  <style name="def:floating-point"          foreground="peach"/>
  <style name="def:keyword"                 foreground="mauve" bold="true"/>
  <style name="def:builtin"                 foreground="red"/>
  <style name="def:variable"                foreground="text"/>

  <style name="def:identifier"              foreground="text"/>
  <style name="def:function"                foreground="blue" italic="true"/>

  <style name="def:statement"               foreground="mauve"/>

  <style name="def:type"                    foreground="yellow" italic="true"/>

  <style name="def:error"                   foreground="base" background="red" bold="true"/>
  <style name="def:note"                    foreground="yellow" bold="true"/>
  <style name="def:net-address-in-comment"  foreground="blue" italic="false" underline="true"/>
  <style name="def:preprocessor"            foreground="yellow"/>

  <style name="def:specials"                foreground="base" background="pink"/>
  <style name="def:boolean"                 foreground="peach"/>

  <style name="c:preprocessor"              foreground="yellow" bold="true"/>
  <style name="c:included-file"             foreground="green"/>
  <style name="c:common-defines"            foreground="blue" bold="true"/>

  <style name="diff:ignore"                 foreground="overlay0"/>
  <style name="diff:added-line"             foreground="green"/>
  <style name="diff:removed-line"           foreground="red"/>
  <style name="diff:changed-line"           foreground="peach"/>
  <style name="diff:diff-file"              foreground="blue" bold="true"/>
  <style name="diff:location"               foreground="sky"/>
  <style name="diff:special-case"           foreground="maroon" bold="true"/>

  <style name="ruby:module-handler"         foreground="mauve"/>
  <style name="ruby:symbol"                 foreground="flamingo"/>
  <style name="ruby:regex"                  foreground="pink"/>

  <style name="sh:function"                 foreground="blue"/>

  <style name="xml:attribute-name"          foreground="yellow"/>
  <style name="xml:element-name"            foreground="blue"/>
  <style name="xml:entity"                  foreground="teal"/>
  <style name="xml:processing-instruction"  bold="true"/>

</style-scheme>
