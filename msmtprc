defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account      animech
host         smtp.office365.com
port         587
from         oskar.grunning@animech.com
user         oskar.grunning@animech.com
passwordeval "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.passwords/mutt/animech.gpg"

account      google
host         smtp.gmail.com
port         587
from         oskargrunning@gmail.com
user         oskargrunning@gmail.com
passwordeval "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.passwords/mutt/google.gpg"
