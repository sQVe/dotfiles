{
  "cleanupPeriodDays": 7,
  "env": {
    "DISABLE_ERROR_REPORTING": "1",
    "DISABLE_NON_ESSNTIAL_MODEL_CALLS": "1",
    "DISABLE_TELEMETRY": "1"
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(cp:*)",
      "Bash(curl:*)",
      "Bash(exa:*)",
      "Bash(fd:*)",
      "Bash(find:*)",
      "Bash(gh api:*)",
      "Bash(gh pr:*)",
      "Bash(git add:*)",
      "Bash(git branch:*)",
      "Bash(git checkout:*)",
      "Bash(git commit:*)",
      "Bash(git diff:*)",
      "Bash(git fetch:*)",
      "Bash(git log:*)",
      "Bash(git merge:*)",
      "Bash(git pull:*)",
      "Bash(git push:*)",
      "Bash(git rebase:*)",
      "Bash(git status:*)",
      "Bash(grep:*)",
      "Bash(jq:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(mv:*)",
      "Bash(npm:*)",
      "Bash(pnpm *)",
      "Bash(rg:*)",
      "Bash(touch:*)",
      "Edit"
    ],
    "deny": []
  }
}
