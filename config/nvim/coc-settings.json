{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "go",
    "html",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "markdown",
    "scss",
    "sh",
    "typescript",
    "typescriptreact",
    "yaml"
  ],
  "coc.source.dictionary.filetypes": [
    "gitcommit",
    "mail",
    "markdown",
    "markdown.mdx",
    "text"
  ],
  "diagnostic.format": "[%source%code] %message",
  "diagnostic.errorSign": "■",
  "diagnostic.hintSign": "■",
  "diagnostic.infoSign": "■",
  "diagnostic.warningSign": "■",
  "diagnostic-languageserver.filetypes": {
    "help": [
      "alex",
      "write-good"
    ],
    "mail": [
      "alex",
      "write-good"
    ],
    "markdown": [
      "alex",
      "write-good"
    ],
    "sh": "shellcheck",
    "text": [
      "alex",
      "write-good"
    ]
  },
  "diagnostic-languageserver.formatFiletypes": {
    "sh": "shfmt"
  },
  "diagnostic-languageserver.formatters": {
    "gofmt": {
      "command": "gofmt",
      "args": [
        "-s"
      ]
    },
    "shfmt": {
      "command": "shfmt",
      "args": [
        "-bn",
        "-ci",
        "-sr",
        "-i",
        "2"
      ]
    }
  },
  "diagnostic-languageserver.linters": {
    "alex": {
      "command": "alex",
      "sourceName": "alex",
      "args": [
        "--stdin"
      ],
      "debounce": 100,
      "isStdout": false,
      "isStderr": true,
      "offsetLine": 0,
      "offsetColumn": 0,
      "formatLines": 1,
      "formatPattern": [
        "^\\s+(\\d+):(\\d+)-(\\d+):(\\d+)\\s+(\\w+)\\s+(.+)$",
        {
          "line": 1,
          "column": 2,
          "endLine": 3,
          "endColumn": 4,
          "securities": 5,
          "message": 6
        }
      ]
    },
    "write-good": {
      "args": [
        "--text=%text",
        "--no-passive"
      ]
    }
  },
  "diagnostic-languageserver.mergeConfig": true,
  "emmet.excludeLanguages": [
    "javascriptreact",
    "markdown",
    "typescriptreact"
  ],
  "git.addedSign.text": "│",
  "git.changeRemovedSign.text": "│",
  "git.changedSign.text": "│",
  "git.removedSign.text": "│",
  "git.topRemovedSign.text": "│",
  "git.addedSign.hlGroup": "GitAdd",
  "git.changeRemovedSign.hlGroup": "GitChangeDelete",
  "git.changedSign.hlGroup": "GitChange",
  "git.removedSign.hlGroup": "GitDelete",
  "git.topRemovedSign.hlGroup": "GitDelete",
  "git.semanticCommit.filetypes": [],
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": [
        "start"
      ],
      "filetypes": [
        "sh"
      ],
      "ignoredRootPaths": [
        "~"
      ]
    }
  },
  "prettier.disableSuccessMessage": true,
  "prettier.onlyUseLocalVersion": true,
  "prettier.requireConfig": true,
  "snippets.priority": 80,
  "snippets.snipmate.enable": false,
  "suggest.completionItemKindLabels": {
    "class": "\uf0e8",
    "color": "\ue22b",
    "constant": "\uf8fe",
    "default": "\uf29c",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "event": "\ufacd",
    "field": "\uf9be",
    "file": "\uf723",
    "folder": "\uf114",
    "function": "\u0192",
    "interface": "\uf417",
    "keyword": "\uf1de",
    "method": "\uf09a",
    "module": "\uf40d",
    "operator": "\u03a8",
    "property": "\ue624",
    "reference": "\ufa46",
    "snippet": "\ue60b",
    "struct": "\ufb44",
    "text": "\ue612",
    "typeParameter": "\uf728",
    "unit": "\uf475",
    "value": "\uf89f",
    "variable": "\ue79b"
  },
  "suggest.languageSourcePriority": 100,
  "suggest.snippetIndicator": " \uf78c"
}
