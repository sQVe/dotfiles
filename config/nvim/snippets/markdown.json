{
  "link": {
    "prefix": "link",
    "body": "[${2}](${1}) $0"
  },
  "image": {
    "prefix": "image",
    "body": "![${1}](${2}) $0"
  },
  "insertStrikethrough": {
    "prefix": "strikethrough",
    "body": "~~${1}~~ $0"
  },
  "insertCodeBlock": {
    "prefix": "codeblock",
    "body": "```${1:language}\n$0\n```"
  },
  "prettierIgnore": {
    "prefix": "prettier-ignore",
    "body": "<!-- prettier-ignore -->\n$0"
  },
  "comment": {
    "prefix": "comment",
    "body": "<!-- $1 -->\n$0"
  },
  "todoComment": {
    "prefix": "todo",
    "body": "<!-- TODO -->\n$0"
  }
}
